{
  "name": "ocr_systems",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix && rm -rf ./dist && mkdir dist && mkdir ./dist/client",
    "build": "npm run prebuild && tsc && cp -a ./src/client/views/. ./dist/client/views && cp -a ./src/client/public/. ./dist/client/public && mkdir ./dist/tmp",
    "prestart": "npm run build",
    "start": "node ./dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "test:lint": "eslint .",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "fs": "0.0.1-security",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "node-tesseract": "^0.2.7",
    "node-tesseract-ocr": "2.0.0",
    "nodemon": "^1.19.1",
    "request": "^2.79.0",
    "typescript": "4.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-syntax-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-stage-3": "^7.8.3",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.35",
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "core-js": "3.0.0-beta.11",
    "eslint": "7.8.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
